{
  "manifest_version": 3,
  "name": "Elasticsearch Create Extension",
  "version": "1.0",
  "description": "PC1의 Elasticsearch에 데이터를 생성하는 크롬 확장 프로그램",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-16.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
